<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net" 
		  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		  xsi:schemaLocation="http://dozer.sourceforge.net http://dozer.sourceforge.net/schema/beanmapping.xsd">

	<configuration>
		<wildcard>true</wildcard>
		<map-null>false</map-null>		
	</configuration>

	<mapping wildcard="true" type="one-way">
		<class-a>org.springframework.data.domain.Page</class-a>
		<class-b>it.ccse.uscite.application.facade.dto.output.PageDTO</class-b>
		<field>
			<a get-method="getNumber">number</a>
			<b>number</b>
		</field>
		<field>
			<a get-method="getNumberOfElements">numberOfElements</a>
			<b>numberOfElements</b>
		</field>
		<field>
			<a get-method="getSize">size</a>
			<b>size</b>
		</field>
		<field>
			<a get-method="getContent">content</a>
			<b set-method="setContent">content</b>
		</field>
	</mapping>
	<mapping wildcard="true" type="one-way">
		<class-a>it.ccse.uscite.application.facade.dto.input.SearchPratiche_InDTO</class-a>
		<class-b>it.ccse.uscite.domain.filter.PraticaFilter</class-b>
		<field>
			<a get-method="getListaIdSettoriAttivita">listaIdSettoriAttivita</a>
			<b set-method="setListaIdSettoriAttivita">listaIdSettoriAttivita</b>
		</field>
		
	</mapping>
	
	<mapping type="bi-directional" wildcard="true" relationship-type="cumulative">
		<class-a>it.ccse.uscite.application.facade.dto.input.AggiornaSemaforiAnagrafica_InDTO</class-a>
		<class-b>it.ccse.uscite.domain.common.ListContainer</class-b>
		<field>
			<a set-method="addElement" type="iterate">content</a>
			<b set-method="addElement" type="iterate">content</b>
			<a-hint>it.ccse.uscite.application.facade.dto.SettoreAttivitaDTO</a-hint>
			<b-hint>it.ccse.uscite.domain.SettoreAttivita</b-hint>
		</field>
	</mapping>
	
	<mapping type="bi-directional" wildcard="true" map-id="praticaContentMap" relationship-type="cumulative">
		<class-a>it.ccse.uscite.domain.common.ListContainer</class-a>
		<class-b>it.ccse.uscite.application.facade.dto.ListContainerDTO</class-b>
		<field>
			<a set-method="addElement" type="iterate">content</a>
			<b set-method="addElement" type="iterate">content</b>
			<a-hint>it.ccse.uscite.domain.PraticaErogazione</a-hint>
			<b-hint>it.ccse.uscite.application.facade.dto.PraticaErogazioneDTO</b-hint>
		</field>
	</mapping>
	
	<mapping type="one-way" wildcard="true" relationship-type="cumulative">
		<class-a>it.ccse.uscite.domain.common.ListContainer</class-a>
		<class-b>it.ccse.uscite.application.facade.dto.output.GetTipiPeriodo_OutDTO</class-b>
		<field>
			<a set-method="addElement" type="iterate">content</a>
			<b set-method="addElement" type="iterate">content</b>
			<a-hint>it.ccse.uscite.domain.TipoPeriodo</a-hint>
			<b-hint>it.ccse.uscite.application.facade.dto.TipoPeriodoDTO</b-hint>
		</field>
	</mapping>
	
