	<mapping map-id="noteDettaglioComitatoFullMappingID" type="one-way">
		<class-a>it.ccse.uscite.domain.ProcessoErogazione</class-a>
		<class-b>it.ccse.uscite.application.facade.dto.NotaPagamentoFullDTO</class-b>
		<field-exclude>
			<a>ordineDelGiorno</a>
			<b>ordineDelGiorno</b>
		</field-exclude>
	</mapping>
	
	<mapping>
			<class-a>it.ccse.uscite.domain.OrdineDelGiorno</class-a>
			<class-b>it.ccse.uscite.application.facade.dto.ComitatoFullDTO</class-b>
