package it.ccse.uscite.domain.common;

import java.util.List;



/**
 * @author vcompagnone
 *
 */
public class ListContainer<T> extends Container<List<T>> {


	public ListContainer(List<T> content) {
		setContent(content);
	}

	public void addElement(T t){
		List<T> content = getContent();
		content.add(t);
	}
	
}
